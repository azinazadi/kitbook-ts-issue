{
  "name": "template",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "kitbook": "vite dev --mode kitbook",
    "kitbook:build": "svelte-kit sync && vite build --mode kitbook",
    "kitbook:build:ci": "cd ../.. && pnpm build:deps && cd packages/kitbook && pnpm package && cd ../template && pnpm kitbook:build",
    "kitbook:preview": "vite preview --mode kitbook",
    "package": "svelte-kit package",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "2.0.0",
    "@sveltejs/kit": "1.15.2",
    "kitbook": "1.0.0-alpha.31",
    "svelte": "^3.58.0",
    "svelte-check": "^3.2.0",
    "tslib": "^2.5.0",
    "typescript": "~4.9.4",
    "vite": "^4.2.1"
  },
  "type": "module"
}
