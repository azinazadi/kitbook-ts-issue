{
  "name": "template",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "kitbook": "vite dev --mode kitbook",
    "kitbook:build": "svelte-kit sync && vite build --mode kitbook",
    "kitbook:build:ci": "cd ../.. && pnpm build:deps && cd packages/kitbook && pnpm package && cd ../template && pnpm kitbook:build",
    "kitbook:preview": "vite preview --mode kitbook",
    "package": "svelte-kit package",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "2.1.0",
    "@sveltejs/kit": "1.20.5",
    "kitbook": "npm:kitbook@1.0.0-alpha.33",
    "svelte": "^4.0.0",
    "svelte-check": "^3.4.4",
    "tslib": "^2.5.3",
    "typescript": "~5.1.3",
    "vite": "^4.3.9"
  },
  "type": "module"
}
