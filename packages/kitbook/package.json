{
  "name": "kitbook",
  "version": "1.0.0-alpha.33",
  "description": "Svelte Component Documentation and Prototyping Workbench built using SvelteKit",
  "scripts": {
    "dev": "vite dev --mode kitbook",
    "build:deps": "cd ../.. && pnpm build:deps && pnpm pieces:package && cd packages/kitbook",
    "build": "pnpm build:deps && vite build --mode kitbook",
    "preview": "vite preview --mode kitbook",
    "package": "svelte-kit sync && svelte-package && publint",
    "package:watch": "svelte-kit sync && svelte-package --watch",
    "prepublishOnly": "vitest run && npm run package",
    "check": "svelte-check --tsconfig ./tsconfig.json --threshold warning --diagnostic-sources js,svelte",
    "check:watch": "svelte-check --tsconfig ./jsconfig.json --threshold warning --diagnostic-sources js,svelte --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "test": "vitest",
    "test:ui": "vitest --ui"
  },
  "type": "module",
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./plugins/vite": {
      "types": "./dist/plugins/vite.d.ts",
      "svelte": "./dist/plugins/vite.js",
      "default": "./dist/plugins/vite.js"
    }
  },
  "typesVersions": {
    ">4.0": {
      "*": [
        "./dist/*.d.ts"
      ]
    }
  },
  "dependencies": {
    "@kitbook/mdsvex-shiki-twoslash": "0.0.3",
    "@kitbook/vite-plugin-kitbook": "0.0.11",
    "svelte-pieces": "2.0.0-next.1",
    "svelte2tsx": "^0.6.16"
  },
  "peerDependencies": {
    "@sveltejs/kit": "^1.13.0",
    "svelte": "^3.55.0 || ^4.0.0"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.83",
    "@sveltejs/adapter-auto": "2.1.0",
    "@sveltejs/kit": "^1.20.5",
    "@sveltejs/package": "^2.1.0",
    "@unocss/svelte-scoped": "^0.53.3",
    "prettier": "^2.8.8",
    "publint": "^0.1.12",
    "svelte": "^4.0.0",
    "svelte-check": "^3.4.4",
    "tslib": "^2.5.3",
    "typescript": "~5.1.3",
    "unocss": "^0.53.3",
    "vite": "^4.3.9",
    "vitest": "^0.32.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jacob-8/kitbook.git",
    "directory": "packages/kitbook"
  },
  "keywords": [
    "Kitbook",
    "Svelte",
    "SvelteKit",
    "Typescript",
    "UI",
    "Component library",
    "Documentation",
    "Prototyping",
    "Workbench",
    "Stories"
  ],
  "author": "Jacob Bowdoin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jacob-8/kitbook/issues"
  },
  "homepage": "https://kitbook.vercel.app"
}