{
  "name": "svelte-pieces",
  "version": "2.0.0-next.2",
  "description": "Useful Svelte UI pieces for building web apps",
  "scripts": {
    "dev": "vite dev --mode kitbook",
    "build": "svelte-kit sync && vite build --mode kitbook",
    "preview": "vite preview --mode kitbook",
    "sync": "svelte-kit sync",
    "package": "svelte-kit sync && svelte-package && publint",
    "package:watch": "svelte-kit sync && svelte-package --watch",
    "prepublishOnly": "svelte-kit sync && vitest run && npm run package",
    "check": "svelte-check --tsconfig ./tsconfig.json --threshold error --diagnostic-sources js,svelte --compiler-warnings a11y-no-static-element-interactions:ignore",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --threshold warning --diagnostic-sources js,svelte --watch",
    "test": "vitest"
  },
  "type": "module",
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "fast-xml-parser": "^3.19.0",
    "he": "^1.2.0"
  },
  "peerDependencies": {
    "svelte": "^3.55.0 || ^4.0.0"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.83",
    "@sveltejs/adapter-auto": "2.1.0",
    "@sveltejs/kit": "1.20.5",
    "@sveltejs/package": "2.1.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/youtube": "^0.0.47",
    "@unocss/svelte-scoped": "^0.53.4",
    "kitbook": "1.0.0-alpha.37",
    "publint": "^0.1.12",
    "svelte": "^4.0.0",
    "svelte-check": "^3.4.4",
    "svelte2tsx": "^0.6.16",
    "tslib": "^2.5.3",
    "typescript": "^5.1.5",
    "unocss": "^0.53.4",
    "vite": "^4.3.9",
    "vitest": "^0.32.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jacob-8/kitbook.git",
    "directory": "packages/svelte-pieces"
  },
  "keywords": [
    "Svelte",
    "SvelteKit",
    "Typescript",
    "UI",
    "Component library",
    "Kitbook"
  ],
  "author": "Jacob Bowdoin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jacob-8/kitbook/issues"
  },
  "homepage": "https://svelte-pieces.vercel.app",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./*": "./dist/*"
  },
  "typesVersions": {
    ">4.0": {
      "index": [
        "./dist/index.d.ts"
      ],
      "*": [
        "./dist/*"
      ]
    }
  }
}