{
  "name": "svelte-pieces",
  "version": "1.0.58",
  "description": "Useful Svelte UI pieces for building web apps",
  "scripts": {
    "dev": "pnpm kitbook",
    "build": "pnpm kitbook:build",
    "preview": "pnpm kitbook:preview",
    "kitbook": "vite dev --mode kitbook",
    "kitbook:build": "vite build --mode kitbook",
    "kitbook:preview": "vite preview --mode kitbook",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "vitest run && npm run package",
    "check": "svelte-check --tsconfig ./tsconfig.json --threshold warning --diagnostic-sources js,svelte",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --threshold warning --diagnostic-sources js,svelte --watch",
    "lint": "prettier --ignore-path .gitignore  --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "test": "vitest"
  },
  "type": "module",
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "fast-xml-parser": "^3.19.0",
    "he": "^1.2.0"
  },
  "peerDependencies": {
    "@sveltejs/kit": "^1.13.0",
    "svelte": "^3.55.0"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.37",
    "@sveltejs/adapter-auto": "2.0.0",
    "@sveltejs/kit": "1.13.0",
    "@sveltejs/package": "2.0.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/youtube": "^0.0.47",
    "kitbook": "1.0.0-alpha.26",
    "publint": "^0.1.9",
    "svelte": "^3.57.0",
    "svelte-check": "^3.1.4",
    "svelte-preprocess-unocss": "^0.0.1",
    "svelte2tsx": "^0.6.10",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4",
    "vite": "^4.2.1",
    "vitest": "^0.29.7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jacob-8/kitbook.git",
    "directory": "packages/svelte-pieces"
  },
  "keywords": [
    "Svelte",
    "SvelteKit",
    "Typescript",
    "UI",
    "Component library",
    "Kitbook"
  ],
  "author": "Jacob Bowdoin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jacob-8/kitbook/issues"
  },
  "homepage": "https://svelte-pieces.vercel.app",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./*": "./dist/*"
  }
}